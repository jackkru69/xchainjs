    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.6",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "autoprefixer": "^9.7.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "react": "^16.11.0",
    "react-clipboard.js": "^2.0.16",

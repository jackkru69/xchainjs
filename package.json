    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "yarn": "^1.19.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.158",
    "axios": "^0.21.0",
